{
  "presets": [
    "babel-preset-stage-0",
    "babel-preset-react",
  ],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    "babel-plugin-transform-class-properties",
    [
      "babel-plugin-root-import",
      {
        "rootPathPrefix": "~",
        "rootPathSuffix": "src/"
      }
    ]
  ]
}
