{
  "name": "rui-kit",
  "version": "0.0.9",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-root-import": "^5.1.0",
    "esayemm-styles": "^2.1.2",
    "eslint": "^4.2.0",
    "eslint-config-esayemm": "^2.0.4",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "history": "^4.6.3",
    "html": "^1.0.0",
    "invariant": "^2.2.2",
    "jbs-fe": "^0.0.28",
    "js-functions": "^0.0.4",
    "kontinuum-route53": "^0.1.3",
    "kontinuum-s3-deploy": "^0.1.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-css-modules": "^4.5.2",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.6.1"
  },
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js . --fix",
    "build": "BABEL_REACT=true NODE_PATH=./src ./node_modules/jbs-fe/bin.js build:package --input src --output build --es-input-file src/index.js --es-output-file build/index.es.js",
    "start": "BABEL_REACT=true NODE_PATH=./example:./src ./node_modules/jbs-fe/bin.js dev --app-index ./example/index.js --html-index ./example/index.html --context ./example --favicon-path ./example/assets/images/cat.jpg",
    "build:example": "BABEL_REACT=true NODE_PATH=./example:./src ./node_modules/jbs-fe/bin.js build --app-index ./example/index.js --html-index ./example/index.html --context ./example --favicon-path ./example/assets/images/cat.jpg --output-path example-build",
    "test": "BABEL_REACT=true ./node_modules/jbs-fe/bin.js test --single-run",
    "test:watch": "BABEL_REACT=true ./node_modules/jbs-fe/bin.js test",
    "prepublish": "npm run build",
    "preversion": "npm run lint && npm run test",
    "version": "npm publish",
    "postversion": "git add . && git push && git push --tags",
    "deploy:s3": "./node_modules/kontinuum-s3-deploy/script.sh --name rui.esayemm.com ./example-build",
    "deploy:route53": "./node_modules/kontinuum-route53/script.sh --name rui.esayemm.com --root esayemm.com",
    "deploy": "npm run build:example && npm run deploy:s3 && npm run deploy:route53"
  },
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "babel": {
    "presets": [
      "./node_modules/jbs-fe/configs/babel-preset-jbs-fe.js"
    ],
    "plugins": [
      [
        "babel-plugin-root-import",
        {
          "rootPathPrefix": "~",
          "rootPathSuffix": "src/"
        }
      ]
    ]
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "tinycolor2": "^1.4.1"
  },
  "files": [
    "build"
  ]
}
